ul.fbits-responsive-carrinho-header li, .fbits-responsive-carrinho-subtotal, .fbits-responsive-carrinho-total
  background transparent!important
  border-top: 1px solid #ccc
  border-bottom: 1px solid #ccc

.fbits-carrinho .btn-success
  background #373535!important
    color: #373535
    text-transform: uppercase
    font-size: 12px
    font-weight: 700

.container-finalizar
  .btn-success
        background: #377f4f!important 
        border 1px solid #377f4f!important

.fbits-responsive-carrinho-item-descricao a
  color #373535!important
  font-weight: 700

.fbits-carrinho a.btn.btn-danger.icon.remover:before, a.arrow:before, a.arrow:after
  color #373535!important

.fbits-carrinho 
  .fbits-responsive-carrinho-items
    border none!important

  .fbits-responsive-carrinho-item-total
    > .currency
      color: #373535
      font-weight: 700

  .precoParcela
    fbits-parcela-juros
      display block

  .fbits-responsive-carrinho-subtotal 
    span
      color: #373535
      text-transform: uppercase
      font-size: 12px
      font-weight 700

  .fbits-responsive-carrinho-total
    div
      color: #373535
      text-transform: uppercase
      font-size: 12px
      font-weight 700

  #naoSeiCep
    display none

  input.btn.btn-success.btnCalcular

body,
html
  font 400 100%/1.4 'Muli', sans-serif!important
  -webkit-font-smoothing: antialiased
  -moz-osx-font-smoothing: grayscale

*
    outline none

.svg-sprites
  display none

body 
    opacity 0
    transition: all 400ms ease-in-out
    padding-top: 165px

.active__body
    opacity: 1

::selection
  background $cor02
  color white

body  
    background: #fff!important
    padding-top 82px!important
    @media(max-width 991px)
      padding-top 82px!important

a,
input
  &:focus
  &:active
    box-shadow none !important
    outline none !important

input[type="text"]
input[type="password"]
    border: 1px solid #ddd
    height: 40px
    padding: 0 10px

.fbits-responsive-carrinho-desconto-input
  input.form-control
    padding: 6px 12px

::-webkit-scrollbar
  height 5px
  width 5px

::-webkit-scrollbar-track
  background-color #f7f7f7

::-webkit-scrollbar-thumb
  background-color #373535
 
.avalie-indique
    margin-top 60px

#divErros
    display: none
img 
  max-width: 100%


.fbits-geral 
  .content
    max-width: 1920px 
 

@media(max-width: 375px)
  .fbits-carrinho 
    .fbits-carrinho-frete-item, .fbits-responsive-carrinho-frete-header, .fbits-responsive-carrinho-frete-valor, 
    .fbits-responsive-carrinho-frete-complemento, .fbits-responsive-carrinho-frete-header, .fbits-responsive-carrinho-frete-valor,
    .fbits-responsive-carrinho-frete-complemento
      font-size: 12px

.slick-dots
  bottom 10px!important

  li
    width 10px!important
    height 10px!important
    border 1px solid #fff!important
    border-radius 50%!important
    background #fff

    button
      font-size 0

  li.slick-active
    background #000

.banner__mid 
    display: flex
    justify-content space-between
    max-width 1920px
    padding 0 30px
    margin 30px auto
    @media(max-width 991px)
        padding 0  
        flex-wrap wrap
    .fbits-banner-item 
      flex  0 0 49%
      overflow hidden

      @media(max-width 991px)
        flex  0 0 100%
        margin-bottom 20px

      img
        transition all 700ms ease-in-out

      &:hover
        img
          transform scale(1.1)


.instagram
  margin 30px 0  
  .showcase__title
    strong
      text-transform none

      &:before
        content '@'

.spot, section.fbits-section-home .spot
  border 1px solid transparent!important

body.fbits-carrinho, body.fbits-login,body.minhaConta 
  padding 0!important

.head-carrinho.headFechamento.container.tyc_container 
    max-width: 100%
    background: #f7f7f7

    &:before, &:after
      display none

.fbits-center-responsivo
  display: flex
  align-items: center
  justify-content: space-between
  max-width: 1330px
  padding: 20px 15px
  margin: 0 auto

  .headerLogo 
      float: none

      img
        width auto

  .pull-right.fbits-telefones
    margin 0 
    background: transparent
    color: #9a9a9a

    span
      display: flex
      align-items: center
      &:after

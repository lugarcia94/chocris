@svg-load star1 url("../assets/svg/star1.svg")
@svg-load money url("../assets/svg/money.svg")
@svg-load sino url("../assets/svg/sino.svg")

@svg-load menumobile url("../assets/svg/menu-mobile.svg")
.header__mainbar-mobile
    background-image svg-inline(menumobile)
    background-size: auto
    background-position: center
    background-repeat: no-repeat
@svg-load arrowmenumobile url("../assets/svg/arrow-menu-mobile.svg")
span.arrowmenu
    background-image svg-inline(arrowmenumobile)
    background-size: auto
    background-position: center
    background-repeat: no-repeat

@svg-load consulta url("../assets/svg/consulta.svg")
@svg-load arrowmenuclose url("../assets/svg/arrow-menu-close.svg")
.header__mainbar-mobile-close
    background-image svg-inline(arrowmenuclose)
    background-size: auto
    background-position: center
    background-repeat: no-repeat
    width: 45px
    height 45px
    position absolute
    top: 0
    left 255px
    @media(min-width : 992px)
        display: none

@svg-load removefilter url("../assets/svg/remove-filter.svg")

$ico
    background-color: transparent
    background-position: center center
    background-repeat: no-repeat

$ico-before
    background-position: center center
    background-repeat: no-repeat
    content: ''
    display: inline-block
    background-position: center center
    background-repeat: no-repeat
    background-size: 100% 100%
    height: 25px
    width: 25px

@svg-load medal url("../assets/svg/medal.svg")
@svg-load store url("../assets/svg/store.svg")
@svg-load creditcard url("../assets/svg/creditcard.svg")
@svg-load barcode url("../assets/svg/barcode.svg")

.header__topbar-item--one
    .header__topbar-icon
        background-image svg-inline(medal)
        width: 17px
        height: 18px
        background-size: auto
        background-repeat: no-repeat
        background-position: center
        margin-right 3px
        display: block
.header__topbar-item--two
    .header__topbar-icon
        background-image svg-inline(store)
        width: 17px
        height: 17px
        background-size: auto
        background-repeat: no-repeat
        background-position: center
        margin-right 2px
        display: block
.header__topbar-item--three
    .header__topbar-icon
        background-image svg-inline(creditcard)
        width: 19px
        height: 17px
        background-size: auto
        background-repeat: no-repeat
        background-position: center
        margin-right 4px
        display: block
.header__topbar-item--four
    .header__topbar-icon
        background-image svg-inline(barcode)
        width: 17px
        height: 14px
        background-size: auto
        background-repeat: no-repeat
        background-position: center
        margin-right 4px
        display: block

@svg-load telephone url("../assets/svg/telephone.svg")
@svg-load whatsapp url("../assets/svg/whatsapp.svg")
.header__topbar-phone--one
    .header__topbar-icon
        background-image svg-inline(telephone)
        width: 17px
        height: 17px
        background-size: auto
        background-repeat: no-repeat
        background-position: center
        margin-right 3px
        display: block
.header__topbar-phone--two
    .header__topbar-icon
        background-image svg-inline(whatsapp)
        width: 17px
        height: 17px
        background-size: auto
        background-repeat: no-repeat
        background-position: center
        margin-right 3px
        display: block

@svg-load lupa url("../assets/svg/lupa.svg")
input.inputSearch
    background-image svg-inline(lupa)
    background-size: auto
    background-position: 3% center
    background-repeat: no-repeat

@svg-load user url("../assets/svg/user.svg")
@svg-load box url("../assets/svg/box.svg")
@svg-load chat url("../assets/svg/chat.svg")
.header__mainbar-account
    a
        display: flex
        align-items: center
    &--icon
        display: block
        width: 13px
        height 15px
        margin-right: 3px
        background-image svg-inline(user)
        background-size: auto
        background-position: center
        background-repeat: no-repeat
.header__mainbar-order
    a
        display: flex
        align-items: center
    &--icon
        display: block
        width: 15px
        height 15px
        margin-right: 3px
        background-image svg-inline(box)
        background-size: auto
        background-position: center
        background-repeat: no-repeat
.header__mainbar-contact
    a
        display: flex
        align-items: center
    &--icon
        display: block
        width: 15px
        height 15px
        margin-right: 3px
        background-image svg-inline(chat)
        background-size: auto
        background-position: center
        background-repeat: no-repeat

@svg-load cart url("../assets/svg/cart.svg")
.header__mainbar-cart
    .carrinho.carrinhoHead
        a
            &:before
                content: ''
                display: flex
                width: 25px
                height 23px
                background-image svg-inline(cart)
                background-size: auto
                background-position: center
                background-repeat: no-repeat
                position relative
                z-index 1

@svg-load facebook url("../assets/svg/facebook.svg")
@svg-load youtube url("../assets/svg/youtube.svg")
@svg-load linkedin url("../assets/svg/linkedin.svg")
.footer__social-link
    &--facebook
        background-image svg-inline(facebook)
        background-size: auto
        background-position: center
        background-repeat: no-repeat
    &--youtube
        background-image svg-inline(youtube)
        background-size: auto
        background-position: center
        background-repeat: no-repeat
    &--linkedin
        background-image svg-inline(linkedin)
        background-size: auto
        background-position: center
        background-repeat: no-repeat

@svg-load right url("../assets/svg/arrow-right.svg")
@svg-load left url("../assets/svg/arrow-left.svg")
.slick-prev.slick-arrow
    background-image svg-inline(left)!important
    background-size: auto!important
    background-position: center!important
    background-repeat: no-repeat!important
    width: 40px
    height 40px
    z-index: 9
    &:before
        display: none
.slick-next.slick-arrow
    background-image svg-inline(right)!important
    background-size: auto!important
    background-position: center!important
    background-repeat: no-repeat!important
    width: 40px
    height 40px
    z-index: 9
    &:before
        display: none

.stay-modal-home .stay-content .slick-arrow.slick-next
    background-image svg-inline(right)!important
    background-size: auto!important
    background-position: center!important
    background-repeat: no-repeat!important

.stay-modal-home .stay-content .slick-arrow.slick-prev
    background-image svg-inline(left)!important
    background-size: auto!important
    background-position: center!important
    background-repeat: no-repeat!important

